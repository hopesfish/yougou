{
  "apps" : [{
    "name"       : "wxgift",
    "script"     : "./bin/www",
    "instances"  : "1",
    "log_date_format"  : "YYYY-MM-DD HH:mm",
    "error_file" : "/var/log/yougou/wxgift-error.log",
    "out_file"   : "/var/log/yougou/wxgift-out.log",
    "pid_file"   : "/var/log/yougou/wxgift.pid",
    "exec_mode"  : "fork",
    "port"       : 3002,
    "env": {
      "NODE_ENV": "online"
    }
  }]
}