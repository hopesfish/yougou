body
    background-color #000
    padding 0
    margin 0
.finddiff-layer
    position relative
    margin auto
    width 960px
    height 320px
    .logo
        position absolute
        left 0px
        top 0px
        width 160px
        height 68px
        background url(/public/images/logo.png?__inline) no-repeat -5px -5px
    .voice
        position absolute
        right 0px
        top 0px
        width 48px
        height 48px
        background url(/public/images/voice.png?__inline) no-repeat 0 0
#finddiff-entry
    display none
    background url(/public/images/bg1.jpg?__inline) no-repeat
    .layer
        display none
    .layer-show
        display block
    .content-wrap
        text-align center
    .title-wrap
        height 97px
    .rule-wrap
        position relative
        top -5px
        height 171px
    .result-wrap
    .rank-wrap
        padding-top 20px
    .button-wrap
        position relative
        top -10px
        text-align center
        .btn
            margin 0 20px
        .notice-btn
            position relative
            top 1px
    .end-button-wrap
        padding-top 10px
        .btn
            margin 3px 20px
#finddiff-helper
    display none
    background url(/public/images/helper-bg.jpg?__inline) no-repeat
#finddiff-game
    background url(/public/images/game-bg.jpg?__inline) no-repeat
    .logo
        top 0px
        width 118px
        height 50px
        background url(/public/images/logo-min.jpg?__inline) no-repeat -5px -5px
        
