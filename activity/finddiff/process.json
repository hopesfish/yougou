{
  "apps" : [{
    "name"       : "finddiff",
    "script"     : "./bin/www",
    "instances"  : "1",
    "log_date_format"  : "YYYY-MM-DD HH:mm",
    "error_file" : "/var/log/yougou/finddiff-error.log",
    "out_file"   : "/var/log/yougou/finddiff-out.log",
    "pid_file"   : "/var/log/yougou/finddiff.pid",
    "exec_mode"  : "fork",
    "port"       : 3001,
    "env": {
      "NODE_ENV": "online"
    }
  }]
}